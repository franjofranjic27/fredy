{
  "name": "@fredy/rag",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsx --env-file=../../.env src/index.ts",
    "ingest": "tsx --env-file=../../.env src/index.ts ingest",
    "sync": "tsx --env-file=../../.env src/index.ts sync",
    "search": "tsx --env-file=../../.env src/index.ts search",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@qdrant/js-client-rest": "^1.9.0",
    "node-cron": "^3.0.3",
    "node-html-parser": "^6.1.13",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.15.0",
    "typescript": "^5.4.5",
    "@vitest/coverage-v8": "^2.0.0",
    "vitest": "^2.0.0"
  }
}
